{
  "schema_version": 1,
  "quilt_loader": {
    "group": "com.diontryban.flourish",
    "id": "flourish",
    "version": "${version}",
    "metadata": {
      "name": "Flourish",
      "description": "Create more small flowers by using bonemeal.",
      "contributors": {
        "Trikzon": "Owner"
      },
      "contact": {
        "homepage": "https://github.com/Trikzon/flourish",
        "issues": "https://github.com/Trikzon/flourish/issues",
        "sources": "https://github.com/Trikzon/flourish"
      },
      "icon": "assets/flourish/icon.png",
      "license": "LGPL-3.0-or-later"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": "com.diontryban.flourish.FlourishQuilt",
      "client_init": "com.diontryban.flourish.client.FlourishClientQuilt"
    },
    "depends": [
      {
        "id": "quilted_fabric_api",
        "versions": ">=7.0.0-"
      },
      {
        "id": "minecraft",
        "versions": "^1.20"
      },
      {
        "id": "ash",
        "versions": "^2.0.0-"
      }
    ]
  },
  "mixin": [
    "flourish.mixins.json",
    "flourish.quilt.mixins.json"
  ]
}
