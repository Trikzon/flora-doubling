{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "homepage": "https://modrinth.com/mod/flourish",
        "sources": "https://github.com/Trikzon/flourish",
        "issues": "https://github.com/Trikzon/flourish/issues"
    },
    "license": "${license}",
    "icon": "${mod_id}.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.diontryban.flourish.FlourishFabric"
        ],
        "client": [
            "com.diontryban.flourish.client.FlourishClientFabric"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "minecraft": "^${minecraft_version}",
        "ash_api": "^20.6.0-beta"
    }
}
  